public class Softdrink{
	private String name;
	private String flavour;
	private int price;
	
	public String getName(){
		return name;
	}
	public void setName(String name){
		this.name=name;
	}
	public String getFlavour(){
		return flavour;
	}
	public void setFlavour(String flavour){
		this.flavour=flavour;
	}
	
}